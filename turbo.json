{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [
        ".env"
    ],
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "lint": {},
        "db:generate": {
            "cache": false
        },
        "db:push": {
            "cache": false
        }
    }
}
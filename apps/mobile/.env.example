# YULA Mobile App Environment Variables
# Copy this file to .env and fill in the values

# API Endpoints
EXPO_PUBLIC_API_URL=http://localhost:8080
EXPO_PUBLIC_AGENTS_URL=http://localhost:8082
EXPO_PUBLIC_WS_URL=ws://localhost:8080

# Authentication (Better Auth)
EXPO_PUBLIC_AUTH_URL=http://localhost:8080/api/auth

# Feature Flags
EXPO_PUBLIC_ENABLE_COUNCIL=true
EXPO_PUBLIC_ENABLE_PAC=true
EXPO_PUBLIC_ENABLE_IMPORT=true
EXPO_PUBLIC_ENABLE_OFFLINE=true

# Analytics (Optional)
EXPO_PUBLIC_POSTHOG_KEY=
EXPO_PUBLIC_SENTRY_DSN=

# Push Notifications (Optional)
EXPO_PUBLIC_EXPO_PROJECT_ID=
